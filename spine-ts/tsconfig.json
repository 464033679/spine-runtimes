{
    "compilerOptions": {
        "module": "none",
        "noImplicitAny": true,
        "removeComments": false,
        "preserveConstEnums": true,
        "outFile": "build/spine-all.js",
        "sourceMap": true,
        "declaration": true
    },
    "include": [
        "core/src/**/*",
        "webgl/src/**/*"
    ],
    "exclude": [
        "build",        
        "build/*.js",
        "build/*.d.ts",
        "build/*.js.map"         
    ]
}